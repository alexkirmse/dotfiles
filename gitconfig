[user]
	name = Alex Kirmse
	email = # fill out
[core]
	excludesfile = /Users/alexkirmse/.gitignore_global
	editor = /usr/local/bin/nvim
[credential]
	helper = osxkeychain
[color]
    diff = auto
    status = auto
    branch = auto
[alias]
    co = checkout
    ci = commit
    st = status
    br = branch
    ba = branch -a
    com = checkout master
    lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)%Creset' --abbrev-commit
[filter "lfs"]
	clean = git lfs clean %f
	smudge = git lfs smudge %f
	required = true
